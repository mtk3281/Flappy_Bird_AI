"class": "Player",
"language": "GDScript",
"path": "res://player/player.gd"
} ]
_global_script_class_icons={
"Aiplayer": "",
"Player": ""
}

[application]

config/name="flappy_bird"
run/main_scene="res://world.tscn"
config/icon="res://assets/textures/angry-birds.png"
config/windows_native_icon="res://assets/textures/angry-birds.png"

[autoload]

Global_var="*res://global.gd"

[display]

